{
    "name": "stiknoltz/filament-scout-table",
    "description": "Filament Scout Table allows the creation of Scout-driven Filament Tables with an Eloquent record base source. ",
    "keywords": [
        "stiknoltz",
        "laravel",
        "filament-scout-table"
    ],
    "homepage": "https://github.com/stiknoltz/filament-scout-table",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolaus Stoltzfus",
            "email": "nik@itsnik.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0",
        "filament/tables": "^2.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "StikNoltz\\FilamentScoutTable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StikNoltz\\FilamentScoutTable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
